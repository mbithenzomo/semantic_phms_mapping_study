@article{ WOS:001375815300021,
Author = {Zheng, Xuzhe and Zhou, Xiaokang and Liang, Wei and Wang, Kevin I-Kai},
Title = {Multitask Correlation Constrained Topological Learning Toward Smart
   Prognostic and Health Management in IoT},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
Year = {2024},
