@article{ WOS:001467451500001,
Author = {Goswami, Chandan and Basak, Aniket and Ghosh, Rajeet and Adhikari,
   Avishek and Sarkar, Pinaki},
Title = {Lightweight authenticated key agreement scheme for IoMT network using
   generalized Chinese Remainder Theorem},
Journal = {COMPUTER NETWORKS},
Year = {2025},
Volume = {263},
Month = {MAY},
Abstract = {Rapid advancement of the Internet of Things (IoT) is revolutionizing
   various sectors of our healthcare being a prime beneficiary. Likelihood
   of viral infections among medical personnel has decreased with the
   introduction of wireless monitoring platforms in the Internet of Medical
   Things network. Recent developments in wireless communications and
   computer systems have further enhanced efficiency, security, and privacy
   of the entire healthcare industry. However, integrating IoT with health
   (e-health) management systems presents several challenges, including
   secure communications insecure channels, authentication, and key
   agreement among participating devices. This manuscript a secure
   three-factor (user, medical server, and sensor device) lightweight
   mutual authenticated key scheme (KAS) based on the Chinese Remainder
   Theorem (CRT) for an arbitrary number of co-primes IoMT network. We
   present a formal semantic security analysis of our proposed protocol
   using the Random (ROR) model for computational security. Moreover,
   simulation results using the Automated of Internet Security Protocols
   and Applications (AVISPA) show that the proposed scheme is safe against
   known active and passive threats. Our proposed scheme is well-designed
   to perform efficiently, reducing computation cost and communication
   cost. Finally, the proposed scheme has significant security and
   performance benefits, making the scheme more efficient, secure, and
