@article{ WOS:001384261600001,
Author = {Moghbelan, Yasamin and Esposito, Alfonso and Zyrianoff, Ivan and
   Spaletta, Giulia and Borgo, Stefano and Masolo, Claudio and Ballarin,
   Fabiana and Seidita, Valeria and Toni, Roberto and Barbaro, Fulvio and
   Di Conza, Giusy and Quartulli, Francesca Pia and Di Felice, Marco},
Title = {A Smart Motor Rehabilitation System Based on the Internet of Things and
   Humanoid Robotics},
Journal = {APPLIED SCIENCES-BASEL},
Year = {2024},
Volume = {14},
